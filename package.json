{
  "name": "react-webpack2-typescript-hmr",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "license": "MIT",
  "devDependencies": {
    "@types/webpack-env": "^1.13.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.2",
    "html-webpack-plugin": "^2.28.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.13.1",
    "ts-loader": "^2.0.0",
    "typescript": "^2.1.5",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-hot-middleware": "^2.16.1"
  },
  "scripts": {
    "start": "webpack-dev-server --watch-poll --https",
    "start-hot": "webpack-dev-server --config webpack.hot.config.js --https",
    "build-react-dev": "webpack && cp ./dist/app.js ./force-app/main/default/staticresources/App.resource",
    "build-react-prod": "webpack --config webpack.prod.config.js && cp ./dist/app.js ./force-app/main/default/staticresources/App.resource",
    "build-sfdx": "sfdx force:source:convert -d ./dist/sfdc-package",
    "build-dev": "npm run build-react-dev && npm run build-sfdx",
    "build-prod": "npm run build-react-prod && npm run build-sfdx",
    "push-dev": "sfdx force:mdapi:deploy -d ./dist/sfdc-package -w 1000",
    "push-prod": "sfdx force:mdapi:deploy -u production -d ./dist/sfdc-package -w 1000",
    "deploy-dev": "npm run build-dev && npm run push-dev",
    "deploy-prod": "npm run build-prod && npm run push-prod",
    "get-token": "sfdx force:org:display"
  },
  "dependencies": {
    "@types/react": "^15.0.6",
    "@types/react-dom": "^0.14.22",
    "axios": "^0.16.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  }
}
